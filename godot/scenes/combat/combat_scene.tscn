script/source = "extends GridContainer

@export var multiple_choices: Array[MultipleChoice]
var used_choices: Array[MultipleChoice]

func appear(multiple_choice: MultipleChoice):
	$\"../RichTextLabel\".text = str(\"[color=black][center]\", multiple_choice.pregunta, \"[/center][/color]\")
	for i in range(0, multiple_choice.posibles_respuestas.size()):
		var boton = get_child(i)
		boton.modulate = Color.WHITE
		boton.get_node(\"RichTextLabel\").text = str(\"[center]\", multiple_choice.posibles_respuestas[i].to_upper(), \"[/center]\")
		boton.pressed.connect(func(): self.on_button_pressed(multiple_choice, multiple_choice.posibles_respuestas[i]))

func on_button_pressed(multiple_choice, respuesta):
	for j in range(0, multiple_choice.posibles_respuestas.size()):
		var boton: Button = get_child(j)
		for connection in boton.pressed.get_connections():
			connection[\"signal\"].disconnect(connection[\"callable\"])
		boton.modulate = Color.GREEN if multiple_choice.posibles_respuestas[j] == multiple_choice.respuesta_correcta else Color.RED
	await disappear()
	var time_to_wait = 0.3
	if(respuesta == multiple_choice.respuesta_correcta):
		pass
		#get_parent().hurt_opponent(30)
	else:
		time_to_wait = 1.2
		get_parent().hurt_player(30)
	await get_tree().create_timer(time_to_wait).timeout
	multiple_choices.erase(multiple_choice)
	used_choices.push_back(multiple_choice)
	if(multiple_choices.is_empty()):
		multiple_choices = used_choices.duplicate()
		used_choices.clear()
	appear(multiple_choices.pick_random())
	$\"../Combat\".appear_menues()
	#appear_menues()

func _ready():
	appear(multiple_choices.pick_random())

func disappear():
	var delay = 0.4
	create_tween().tween_property(
		$\"../ButtonDisable3Slides\",
		\"position:x\",
		-700.0,
		0.7
	).as_relative().set_delay(delay).finished
	create_tween().tween_property(
		$\"../RichTextLabel\",
		\"position:x\",
		-700.0,
		0.7
	).as_relative().set_delay(delay).finished
	create_tween().tween_property(
		$\"../OpponentTurn\",
		\"position:y\",
		782,
		0.7
	).set_trans(Tween.TRANS_ELASTIC).set_delay(delay).finished
	await create_tween().tween_property(
		self,
		\"position:x\",
		-700.0,
		0.7
	).as_relative().set_delay(delay).finished


func appear_menues():
	create_tween().tween_property(
		$\"../ButtonDisable3Slides\",
		\"position:x\",
		700.0,
		0.7
	).as_relative()
	create_tween().tween_property(
		$\"../RichTextLabel\",
		\"position:x\",
		700.0,
		0.7
	).as_relative()
	create_tween().tween_property(
		$\"../OpponentTurn\",
		\"position:y\",
		582,
		0.7
	).set_trans(Tween.TRANS_ELASTIC).finished
	await create_tween().tween_property(
		self,
		\"position:x\",
		700.0,
		0.7
	).as_relative().finished
"

