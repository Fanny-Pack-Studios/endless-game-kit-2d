script/source = "extends VBoxContainer


# Called when the node enters the scene tree for the first time.
func _ready():
	for child in get_children():
		child.pressed.connect(func(): self.reorder_children())
	
func reorder_children():
	await disappear()
	await get_tree().create_timer(1.0).timeout
	$\"../Minigame\".appear_menues()
	return
	get_children().map(func(child):
		move_child(child, randf_range(0, get_child_count() - 1))
		child.pop()
	)

func disappear():
	var delay = 0.4
	create_tween().tween_property(
		self,
		\"position:x\",
		700.0,
		0.7
	).as_relative().set_delay(delay)
	create_tween().tween_property(
		$\"../YourTurn\",
		\"position:y\",
		782.0,
		0.7
	).set_trans(Tween.TRANS_ELASTIC).set_delay(delay).finished

func appear_menues():
	var delay = 0.4
	create_tween().tween_property(
		$\"../YourTurn\",
		\"position:y\",
		582,
		0.7
	).set_trans(Tween.TRANS_ELASTIC).set_delay(delay).finished
	create_tween().tween_property(
		self,
		\"position:x\",
		-700.0,
		0.7
	).as_relative().set_delay(delay).finished
"

