script/source = "extends Node2D

func _ready():
	$InventoryButton.pressed.connect(self.toggle_inventory)
	Inventory.item_selected.connect(func(item):
		Dialogue.say_line(\"Arturo\", \"Es %s\" % Item.name_of(item))
	)


func _unhandled_input(event: InputEvent):
	if event.is_action_pressed(\"toggle_inventory\"):
		toggle_inventory()

func toggle_inventory():
	if Inventory.is_open():
		Inventory.close()
	else:
		Inventory.open()
"

