[sub_resource type="GDScript" id="GDScript_5vx8v"]
script/source = "extends NPC

func interact_with(_player):
	if Inventory.is_empty():
		await say(\"Te doy 3 bolsas de %s por 2 %s\" % [gold(), pumpkin()])
		await say(\"Vuelve con algo para negociar\")
		return

	say(\"Te doy 3 bolsas de %s por 2 %s\" % [gold(), pumpkin()])
	var item = await Inventory.choose_item()
	if item == null:
		await say(\"No me diste nada :(\")
	elif(item == Item.Type.Calabaza):
		if(Inventory.amount_of(Item.Type.Calabaza) < 2):
			await say(\"No tienes suficientes %ss\" % pumpkin())
		else:
			Inventory.remove_item(Item.Type.Calabaza)
			Inventory.remove_item(Item.Type.Calabaza)
			Inventory.add_item(Item.Type.Dinero)
			Inventory.add_item(Item.Type.Dinero)
			Inventory.add_item(Item.Type.Dinero)
			await say(\"Un placer hacer negocios.\")
	else:
		await say(\"¡Quería %s, no %s!\" % [pumpkin(), Item.english_name_of(item)])
	
func say(line: String):
	await Dialogue.say_line(\"[color=purple]Brujo[/color]\", line)

func pumpkin() -> String:
	return \"[hint=calabaza][color=dark_orange]Pumpkin[/color][/hint]\"

func gold() -> String:
	return \"[hint=dinero][color=gold]Money[/color][/hint]\"
"

